package swt6.soccer.domain;

import javax.persistence.*;

@Entity
public class Bet {
	@Id
    @GeneratedValue
    private Long id;

    public Long getId() {
		return id;
	}
    
	@ManyToOne
    @JoinColumn(name = "NAME_ID")
    private SoccerBetSystemUser soccerBetSystemUser;
    
	@ManyToOne
    @JoinColumn(name = "NAME_ID")
    private Game game;

    private int betGoalsA;
    private int betGoalsB;
    
	public int getBetGoalsA() {
		return betGoalsA;
	}
	public void setBetGoalsA(int betGoalsA) {
		this.betGoalsA = betGoalsA;
	}
	
	public int getBetGoalsB() {
		return betGoalsB;
	}
	public void setBetGoalsB(int betGoalsB) {
		this.betGoalsB = betGoalsB;
	}
    public Game getGame() {
		return game;
	}
	public void setGame(Game game) {
		this.game = game;
	}
	public void setId(Long id) {
		this.id = id;
	}
}
