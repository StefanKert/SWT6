package swt6.soccer.domain;

import lombok.Data;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;

/**
 * Team entity
 */
@Entity
@Data
public class Team {

    @Id
    @GeneratedValue
    private Long id;

    private String name;

    public Team(String name){
        this.setName(name);
    }

    public Team() {
    }

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}
